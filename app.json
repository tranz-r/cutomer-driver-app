{
  "expo": {
    "name": "TranzrMoves",
    "slug": "tranzr-moves",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/adaptive-icon.png",
    "scheme": "tranzr-moves",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/icons/ios-light.png"
      },
      "bundleIdentifier": "com.vavy.tranzrmoves",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs access to camera to take photos and record videos of items for smart detection.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images and videos for smart detection.",
        "NSMicrophoneUsageDescription": "This app needs access to microphone to record videos with audio.",
        "UIBackgroundModes": ["audio"],
        "UIRequiresPersistentWiFi": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "GoogleSignIn",
            "CFBundleURLSchemes": ["com.googleusercontent.apps.53650588253-1c6dq2rqqi2baq2mumnef6sucukgb0pa"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vavy.tranzrmoves"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to photo library to select images and videos for smart detection.",
          "cameraPermission": "This app needs access to camera to take photos and record videos of items for smart detection."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.vavy.tranzrmoves",
          "enableGooglePay": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.53650588253-1c6dq2rqqi2baq2mumnef6sucukgb0pa"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "73745d73-b374-402a-9111-efff9deb5a3f"
      }
    },
    "owner": "vavy"
  }
}
